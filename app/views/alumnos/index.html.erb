<p id="notice"><%= notice %></p>

<h1>Alumnos</h1>

<table>
  <thead>
    <tr>
      <th>Clave</th>
      <th>Curp</th>
      <th>Nombre</th>
      <th>Apellido paterno</th>
      <th>Apellido materno</th>
      <th>Correo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alumnos.each do |alumno| %>
      <tr>
        <td><%= alumno.clave %></td>
        <td><%= alumno.curp %></td>
        <td><%= alumno.nombre %></td>
        <td><%= alumno.apellido_paterno %></td>
        <td><%= alumno.apellido_materno %></td>
        <td><%= alumno.correo %></td>
        <td><%= link_to 'Mostrar', alumno %></td>
        <td><%= link_to 'Editar', edit_alumno_path(alumno) %></td>
        <td><%= link_to 'Eliminar', alumno, method: :delete, data: { confirm: 'Esta seguro que desea eliminar al Alumno?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Crear Alumno', new_alumno_path %>
