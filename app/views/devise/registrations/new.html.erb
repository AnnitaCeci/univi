<main class="conteiner-vertical-center">
  <div class="children" align="center">
    
    <div id="logotipo1">
        <img src="/assets/usuario-fe70503caf32280699f90f40bee2b05cebfec8c7f2a97a904495cc49e42a2295.png" style="width: 150px" class="square" alt="Usuario" />
    </div>

  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

    <div class="input-field col s12 center-align">
      <%= f.label :Correo %><br />
      <%= f.email_field :email, autofocus: true %>
     </div>

    <div class="input-field col s12">
      <%= f.label :Contraseña %>
      <% if @minimum_password_length %>
      <% end %><br />
      <%= f.password_field :password, autocomplete: "off" %>
    </div>

    <div class="input-field col s12">
      <%= f.label :Confirmar_contraseña %><br />
      <%= f.password_field :password_confirmation, autocomplete: "off" %>
   </div>
   <br>
    <div class="btn waves-effect waves-light actions">
      <%= f.submit "Registrar" %>
      <i class="material-icons right">send</i>
    </div>
    <% end %>

    <a href="/login">Iniciar sesión</a><br />

  </div>
</main>




