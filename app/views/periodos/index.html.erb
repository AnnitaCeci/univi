<p id="notice"><%= notice %></p>

<h1>Periodos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Inicio</th>
      <th>Fin</th>
      <th>Tipo</th>
      <th>Current</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @periodos.each do |periodo| %>
      <tr>
        <td><%= periodo.nombre %></td>
        <td><%= periodo.inicio %></td>
        <td><%= periodo.fin %></td>
        <td><%= periodo.tipo %></td>
        <td><%= periodo.current %></td>
        <td><%= link_to 'Show', periodo %></td>
        <td><%= link_to 'Edit', edit_periodo_path(periodo) %></td>
        <td><%= link_to 'Destroy', periodo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Periodo', new_periodo_path %>
