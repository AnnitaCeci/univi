<p id="notice"><%= notice %></p>

<h1>Plan Estudios</h1>

<table>
  <thead>
    <tr>
      <th>Clave</th>
      <th>Semestre</th>
      <th>Inicio vigencia</th>
      <th>Fin vigencia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plan_estudios.each do |plan_estudio| %>
      <tr>
        <td><%= plan_estudio.clave %></td>
        <td><%= plan_estudio.semestre %></td>
        <td><%= plan_estudio.inicio_vigencia %></td>
        <td><%= plan_estudio.fin_vigencia %></td>
        <td><%= link_to 'Show', plan_estudio %></td>
        <td><%= link_to 'Edit', edit_plan_estudio_path(plan_estudio) %></td>
        <td><%= link_to 'Destroy', plan_estudio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plan Estudio', new_plan_estudio_path %>
